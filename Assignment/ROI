# Function to calculate average ROI
def calculate_roi(starting_prices, final_prices):
    if len(starting_prices) != len(final_prices):
        print("Error: Lists must be the same length.")
        return

    total_roi = 0
    num_stocks = len(starting_prices)
    for i in range(num_stocks):
        starting = starting_prices[i]
        final = final_prices[i]
        roi = ((final - starting) / starting) * 100
        total_roi = total_roi + roi
    average_roi = total_roi / num_stocks
    print("Average ROI:", round(average_roi, 2),"%")

# Example input
starting_prices = [100, 120, 130]
final_prices = [110, 125, 140]

# Call the function
calculate_roi(starting_prices, final_prices)
