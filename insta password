def validate_password(val):
    if(len(val)<8):
        print("invalid count")
    elif(val.isalpha()!=False):
        print("invalid alp")
    elif(val.isdigit()!=False):
        print("invalid digit")
    elif(val.count("@")==0 and val.count("#")==0):
        print("invalid special")    
    elif(val.count("")>0):
        print("invalid special")
    elif(val.find(" ")>0):
        print("no space")
    else:
        print("valid password")
    
validate_password("Qpassword#@1")