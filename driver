import math
def near_dist(x1,y1,x2,y2,x3,y3):
    p1 = (x1,y1)
    d1 = (x2,y2)
    d2 = (x3,y3)

    dx = math.sqrt((x2-x1)**2 + (y2-y1)**2)
    dy = math.sqrt((x2-x1)**2 + (y2-y1)**2)
    if d1 < d2:
        print("the driver 1 is assigned: ", d1)
    else:
         print("the driver 2 is assigned: ", d2)
    
near_dist(1,2,2,3,8,11)
